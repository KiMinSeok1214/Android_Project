            android:name=".ShowAllActivity"
            android:exported="false" />
        <activity
            android:name=".BaseActivity"
            android:exported="false" />
        <activity
            android:name=".HashtagActivity"
            android:exported="false" />
        <activity
            android:name=".CombineHashtag"
            android:exported="false" />
        <activity
            android:name=".ModifyHashtag"
            android:exported="false" />

        <activity
            android:name=".CategoryActivity"
            android:exported="false" />
        <activity
            android:name=".ModifyCategory"
            android:exported="false" />
        <activity
            android:name=".TestHashtagActivity"
            android:exported="false" />
        <activity
            android:name=".TestCategoryActivity"
            android:exported="false" />
    </application>
</manifest>